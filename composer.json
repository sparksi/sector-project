{
    "name": "sparksinteractive/sector-project",
    "type": "project",
    "description": "A composer-based Drupal project template using Sector.",
    "homepage": "https://www.sector.org.nz",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sparks Interactive",
            "role": "Development"
        }
    ],
    "require": {
        "composer/installers": "^2.2",
        "drupal/core-composer-scaffold": "^10",
        "drush/drush": "^11",
        "oomphinc/composer-installers-extender": "^2.0",
        "sparksinteractive/sector-distribution": "10.0.x@dev"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "repositories": {
        "packagist.drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset.packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
